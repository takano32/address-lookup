{
  "name": "Address Lookup",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Lookup selected text as address",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}